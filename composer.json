{
    "name": "onekone/lore",
    "description": "Helper plugin for documenting Laravel projects",
    "type": "library",
    "require": {
        "zircote/swagger-php": "^5",
        "php": ">=8.1",
        "phpstan/phpdoc-parser": "^1",
        "laravel/framework": ">=10"
    },
    "autoload": {
        "psr-4": {
            "Onekone\\Lore\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Onekone\\Lore\\Providers\\LoreSwaggerProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "onekone",
            "email": "alexs0929@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^12.1"
    }
}
